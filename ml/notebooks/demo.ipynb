   "source": [
    "# IntelGraph ML Demo Notebook\n",
    "\n",
    "This notebook demonstrates how to train and use a simple Graph Neural Network (GNN) for link prediction and anomaly detection using PyTorch Geometric, leveraging the IntelGraph ML service components."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import json\n",
    "import torch\n",
    "import networkx as nx\n",
    "from torch_geometric.data import Data\n",
    "from ml.app.models.gnn_model import GNNLinkPredictor, GNNAnomalyDetector\n",
    "from ml.app.models.gnn import GNNModelManager\n",
    "from ml.app.models.gnn_trainer import GNNDataProcessor, GNNTrainer\n",
    "\n",
    "# Load demo graph data\n",
    "with open('../data/demo_graph.json', 'r') as f:\n",
    "    demo_graph_data = json.load(f)\n",
    "\n",
    "# Convert to NetworkX graph\n",
    "G = nx.Graph()\n",
    "for edge in demo_graph_data['edges']:\n",
    "    G.add_edge(edge['source'], edge['target'])\n",
    "\n",
    "# Prepare node features\n",
    "node_features = {node['id']: torch.tensor(node['features'], dtype=torch.float) for node in demo_graph_data['nodes']}\n",
    "\n",
    "# Convert to PyTorch Geometric Data object\n",
    "pyg_data = GNNDataProcessor.networkx_to_pyg(G, node_features=node_features)\n",
    "\n",
    "print(pyg_data)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Initialize GNN Model Manager\n",
    "gnn_manager = GNNModelManager(model_dir='../models') # Use a local models directory\n",
    "\n",
    "# --- Link Prediction Demo ---\n",
    "print(\"\\n--- Link Prediction Demo ---\")\n",
    "model_name_lp = \"demo_link_predictor\"\n",
    "node_feature_dim_lp = pyg_data.x.size(1)\n",
    "\n",
    "# Create and train a Link Prediction model\n",
    "model_lp = gnn_manager.create_model(\n",
    "    model_name_lp,\n",
    "    node_feature_dim=node_feature_dim_lp,\n",
    "    model_type='graphsage',\n",
    "    task_type='link_prediction',\n",
    "    hidden_dim=32,\n",
    "    output_dim=16\n",
    ")\n",
    "\n",
    "trainer_lp = GNNTrainer(model_lp)\n",
    "train_data_lp, val_data_lp, test_data_lp = GNNDataProcessor.create_link_prediction_data(pyg_data)\n",
    "\n",
    "training_results_lp = trainer_lp.train(\n",
    "    train_loader=[train_data_lp],\n",
    "    val_loader=[val_data_lp],\n",
    "    task_type='link_prediction',\n",
    "    num_epochs=10\n",
    ")\n",
    "print(\"Link Prediction Training Results:\", training_results_lp)\n",
    "\n",
    "# Save the trained model\n",
    "gnn_manager.save_model(model_name_lp)\n",
    "\n",
    "# Load the model and make predictions\n",
    "loaded_model_lp = gnn_manager.load_model(model_name_lp)\n",
    "\n",
    "candidate_edges = [\n",
    "    ['node1', 'node5'], # Existing edge\n",
    "    ['node1', 'node3'], # Non-existing edge\n",
    "    ['node2', 'node4']  # Non-existing edge\n",
    "]
    "\n",
    "node_mapping = {node: i for i, node in enumerate(G.nodes())}\n",
    "edge_candidates_pyg = torch.tensor([[node_mapping[u], node_mapping[v]] for u,v in candidate_edges], dtype=torch.long).t()\n",
    "\n",
    "pyg_data_pred_lp = pyg_data.clone()\n",
    "pyg_data_pred_lp.edge_label_index = edge_candidates_pyg\n",
    "\n",
    "predictions_lp = gnn_manager.predict(model_name_lp, pyg_data_pred_lp)\n",
    "print(\"Link Prediction Scores:\", predictions_lp['predictions'].tolist())"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# --- Anomaly Detection Demo ---\n",
    "print(\"\\n--- Anomaly Detection Demo ---\")\n",
    "model_name_ad = \"demo_anomaly_detector\"\n",
    "node_feature_dim_ad = pyg_data.x.size(1)\n",
    "\n",
    "# Create and train an Anomaly Detection model\n",
    "model_ad = gnn_manager.create_model(\n",
    "    model_name_ad,\n",
    "    node_feature_dim=node_feature_dim_ad,\n",
    "    model_type='gat',\n",
    "    task_type='anomaly_detection',\n",
    "    hidden_dim=32,\n",
    "    output_dim=16\n",
    ")\n",
    "\n",
    "trainer_ad = GNNTrainer(model_ad)\n",
    "\n",
    "# For anomaly detection training, we need labels (normal/anomaly)\n",
    "# Let's assume node1, node2 are normal, node3, node4, node5 are potential anomalies\n",
    "node_labels_ad = {\n",
    "    'node1': 0, # Normal\n",
    "    'node2': 0, # Normal\n",
    "    'node3': 1, # Anomaly\n",
    "    'node4': 1, # Anomaly\n",
    "    'node5': 1  # Anomaly\n",
    "}\n",
    "pyg_data_ad = GNNDataProcessor.networkx_to_pyg(G, node_features=node_features, node_labels=node_labels_ad)\n",
    "\n",
    "training_results_ad = trainer_ad.train(\n",
    "    train_loader=[pyg_data_ad],\n",
    "    val_loader=[pyg_data_ad],\n",
    "    task_type='anomaly_detection',\n",
    "    num_epochs=10\n",
    ")\n",
    "print(\"Anomaly Detection Training Results:\", training_results_ad)\n",
    "\n",
    "# Save the trained model\n",
    "gnn_manager.save_model(model_name_ad)\n",
    "\n",
    "# Load the model and make predictions\n",
    "loaded_model_ad = gnn_manager.load_model(model_name_ad)\n",
    "\n",
    "predictions_ad = gnn_manager.predict(model_name_ad, pyg_data_ad)\n",
    "print(\"Anomaly Scores:\", predictions_ad['predictions'].tolist())"
   ]
  },
